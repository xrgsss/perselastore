{
  "version": 2,
  "builds": [
    { "src": "dist-static/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/dist-static/index.html" },
    { "src": "/produk/(.+)", "dest": "/dist-static/produk/detail/index.html" },
    { "src": "/orders/(.+)", "dest": "/dist-static/orders/detail/index.html" },
    { "src": "/(.*)", "dest": "/dist-static/$1" }
  ]
}
